<div class="container">
  <div class="paginate-links"><%= will_paginate @matches %></div>
  <div class="col-sm-3"></div>
  <div class="col-sm-6 match centered">
    <% @matches.each do |match| %>
      <h4><%= match.name %></h4>
      <image src="<%= match.picture_url %>" class="pics"></image>
      <p><%= match.description %></p>
      <h5>Languages</h5>
      <% match.languages.each do |language| %>
        <p><%= language.name %></p>
      <% end %>
    <div class="col-sm-3">
      <%= link_to matches_path(match_uid: match.uid, like: 0), method: :PUT do %>
        <i class="fa fa-times fa-5x" alt="no"></i>
      <% end %>
    </div>
    <div class="col-sm-3 pull-right">
      <%= link_to matches_path(match_uid: match.uid, like: 1), method: :PUT do %>
        <%#<i class="fa fa-check fa-5x" alt="yes"></i>%>
        like me
      <% end %>
    </div>
    <% end %>
  </div>
</div>
